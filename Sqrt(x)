class Solution {
public:
    int mySqrt(int x) {
       
         for(double i=1;i<=x;i++){
            if(i*i==x){
            return i;
            }
            
            else if (i*i>x){
                int b=(int) i;
            return b-1;
            }
        }
        return 0;
    }
};
